<?xml version="1.0" encoding="UTF-8"?>
<form method="post" enctype="multipart/form-data" action="" title="" label="AUTH_PROFILE_EDIT_TITLE" ajax="validate">
    <fieldgroup name="main">
        <field name="username" type="text" required="0" disabled="1">
            <title>au_lb_uname</title>
        </field>
        <field name="email" type="text" required="1">
            <title>au_lb_email</title>
            <patterns>
                <pattern preset="length" message="frm_err_str_range"><item>5</item><item>120</item></pattern>
                <pattern preset="email" message="frm_err_email" />
            </patterns>
        </field>
        <field name="icq" type="text">
            <title>au_lb_icq</title>
        </field>
        <field name="skype" type="text">
            <title>au_lb_skype</title>
        </field>
        <field name="delimiter" type="delimiter">
            <title>au_lb_pass_IDENTIFICATION</title>
            <description>au_lb_pass_TO_SAVE</description>
        </field>
        <field name="current_password" type="password" required="0">
            <title>au_err_currpass</title>
        </field>
        <field name="delimiter" type="delimiter">
            <title>AUTH_LB_CHANGE_PASSWORD</title>
            <description>AUTH_LB_CHANGE_PASSWORD_DESCR</description>
        </field>
        <field name="password" type="password">
            <title>AUTH_LB_NEW_PASSWORD</title>
        </field>
        <field name="cpassword" type="password" linkedto="password">
            <title>AUTH_REG_CPASSWORD</title>
        </field>
        <field type="buttonset">
            <title />
            <buttons>
                <button type="submit" title="frm_submit" />
            </buttons>
        </field>
    </fieldgroup>
</form>
