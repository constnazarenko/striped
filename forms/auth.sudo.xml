<?xml version="1.0" encoding="UTF-8"?>
<form method="post" action="" title="" reload="1" ajax="none" label="au_sudo">
    <fieldgroup name="main">
        <field name="username" type="text" required="1" autocomplete="ajax/usercomplete" cyr2lat="1">
            <patterns>
                <pattern preset="minlength" message="frm_err_str_min"><item>1</item></pattern>
                <pattern preset="maxlength" message="frm_err_str_max"><item>20</item></pattern>
            </patterns>
            <title>au_lb_uname</title>
        </field>
        <field type="buttonset">
            <buttons>
                <button type="submit" name="submit" title="au_login" />
                <link href="#" title="frm_cancel" onclick="window.parent.$('.overclose').trigger('click'); return false;" class="red"/>
            </buttons>
        </field>
    </fieldgroup>
</form>